                    <TextBlock Grid.Column="0" Grid.Row="2" Name="tbHouseSystem" Text="tbHouseSystem" Margin="6"/>
                    <ComboBox Grid.Column="1" Grid.Row="2" Margin="6" Name="comboHouseSystem" Text="" SelectionChanged="HouseSystemChanged" IsEditable="True" IsReadOnly="True"/>
                    <TextBlock Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="2" Name="tbZodiacTypeExpl" Text="tbZodiacTypeExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="4" Name="tbZodiacType" Text="tbZodiacType" Margin="6"/>
                    <ComboBox Grid.Column="1" Grid.Row="4" Margin="6" Name="comboZodiacType" Text="" SelectionChanged="ZodiacTypeChanged" IsEditable="True" IsReadOnly="True"/>
                    <TextBlock Grid.Column="0" Grid.Row="5" Grid.ColumnSpan="2" Name="tbAyanamshaExpl" Text="tbAyanamshaExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="6" Name="tbAyanamsha" Text="tbAynamsha" Margin="6"/>
                    <ComboBox Grid.Column="1" Grid.Row="6" Margin="6" Name="comboAyanamsha" Text="" SelectionChanged="AyanamshaChanged" IsEditable="True" IsReadOnly="True"/>
                    <TextBlock Grid.Column="0" Grid.Row="7" Grid.ColumnSpan="2" Name="tbObserverPosExpl" Text="tbObserverPosExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="8" Name="tbObserverPosition" Text="tbObserverPosition" Margin="6"/>
                    <ComboBox Grid.Column="1" Grid.Row="8" Margin="6" Name="comboObserverPos" Text="" SelectionChanged="ObserverPosChanged" IsEditable="True" IsReadOnly="True"/>
                    <TextBlock Grid.Column="0" Grid.Row="9" Grid.ColumnSpan="2" Name="tbProjectionTypeExpl" Text="tbProjectionTypeExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="10" Name="tbProjectionType" Text="tbProjectionType" Margin="6"/>
                    <ComboBox Grid.Column="1" Grid.Row="10" Margin="6" Name="comboProjectionType" Text="" SelectionChanged="ObserverPosChanged" IsEditable="True" IsReadOnly="True"/>
