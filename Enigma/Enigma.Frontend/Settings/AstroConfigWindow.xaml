                    <TextBlock Grid.Column="2" Grid.Row="6" Name="tbTextMercury" Text="Mercury" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="6" Name="MercuryFactor" Text="1.0" Height="16.0" VerticalAlignment="Center" />
                    <!-- Venus -->
