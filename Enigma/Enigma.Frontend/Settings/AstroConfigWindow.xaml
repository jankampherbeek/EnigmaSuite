                    <TextBlock Grid.Column="2" Grid.Row="5" Name="tbTextMoon" Text="Moon" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="5" Name="MoonFactor" Text="1.0" Height="16.0" VerticalAlignment="Center" />
