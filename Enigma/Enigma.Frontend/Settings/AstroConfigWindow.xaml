                    <TextBlock Grid.Column="2" Grid.Row="4" Name="tbTextSun" Text="Sun" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="4" Name="SunFactor" Text="1.0" Height="16.0" VerticalAlignment="Center" />
                    <!-- Moon -->
