<Window x:Class="Enigma.Frontend.Settings.AstroConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enigma.Frontend.Settings"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="AstroConfigWindow" Height="720" Width="640">
    <Grid Margin="6">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Name="FormTitle" Text="Form Title" Margin="6" Style="{StaticResource titleStyle}"/>

        <TabControl Grid.Row="1">
            <TabItem Name="tabGeneral" Header="TabGeneral">
                <Grid Margin="6">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="25*"/>
                        <ColumnDefinition Width="25*"/>
                        <ColumnDefinition Width="25*"/>
                        <ColumnDefinition Width="25*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="6"/>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="6"/>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="6"/>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="6"/>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="6"/>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="30"/>


                    </Grid.RowDefinitions>
                    
                    <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4" Name="TitleGeneral" Text="Title General" Margin="6" Style="{StaticResource subTitleStyle}"/>

                    <Rectangle Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="4"  Grid.RowSpan="2" />
                    <TextBlock Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="4" Name="tbHouseSystemExpl" Text="tbHouseSystemExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="2" Name="tbHouseSystem" Text="tbHouseSystem" FontWeight="Bold" Margin="6"/>
                    <ComboBox Grid.Column="2" Grid.Row="2" Grid.ColumnSpan="2"  Margin="6" Name="comboHouseSystem" Text="" IsEditable="True" IsReadOnly="True"/>

                    <Rectangle Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="4"  Grid.RowSpan="2"/>
                    <TextBlock Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="4" Name="tbZodiacTypeExpl" Text="tbZodiacTypeExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="5" Grid.ColumnSpan="2" Name="tbZodiacType" Text="tbZodiacType" FontWeight="Bold" Margin="6"/>
                    <ComboBox Grid.Column="2" Grid.Row="5" Grid.ColumnSpan="2"  Margin="6" Name="comboZodiacType" Text="" SelectionChanged="ZodiacTypeChanged" IsEditable="True" IsReadOnly="True"/>

                    <Rectangle Grid.Column="0" Grid.Row="7" Grid.ColumnSpan="4"  Grid.RowSpan="2"/>
                    <TextBlock Grid.Column="0" Grid.Row="7" Grid.ColumnSpan="4" Name="tbAyanamshaExpl" Text="tbAyanamshaExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="8" Grid.ColumnSpan="2" Name="tbAyanamsha" Text="tbAynamsha" FontWeight="Bold" Margin="6"/>
                    <ComboBox Grid.Column="2" Grid.Row="8" Grid.ColumnSpan="2" Margin="6" Name="comboAyanamsha" Text="" SelectionChanged="AyanamshaChanged" IsEditable="True" IsReadOnly="True"/>

                    <Rectangle Grid.Column="0" Grid.Row="10" Grid.ColumnSpan="4"  Grid.RowSpan="2"/>
                    <TextBlock Grid.Column="0" Grid.Row="10" Grid.ColumnSpan="4" Name="tbObserverPosExpl" Text="tbObserverPosExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="11" Grid.ColumnSpan="2" Name="tbObserverPos" Text="tbObserverPos" FontWeight="Bold" Margin="6"/>
                    <ComboBox Grid.Column="2" Grid.Row="11" Grid.ColumnSpan="2" Margin="6" Name="comboObserverPos" Text="" IsEditable="True" IsReadOnly="True"/>

                    <Rectangle Grid.Column="0" Grid.Row="13" Grid.ColumnSpan="4"  Grid.RowSpan="2"/>
                    <TextBlock Grid.Column="0" Grid.Row="13" Grid.ColumnSpan="4" Name="tbProjectionTypeExpl" Text="tbProjectionTypeExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="14" Grid.ColumnSpan="2" Name="tbProjectionType" Text="tbProjectionType" FontWeight="Bold" Margin="6"/>
                    <ComboBox Grid.Column="2" Grid.Row="14" Grid.ColumnSpan="2" Margin="6" Name="comboProjectionType" Text="" IsEditable="True" IsReadOnly="True"/>

                    <Rectangle Grid.Column="0" Grid.Row="16" Grid.ColumnSpan="4"  Grid.RowSpan="2"/>
                    <TextBlock Grid.Column="0" Grid.Row="16" Grid.ColumnSpan="4" Name="tbBaseOrbExpl" Text="tbBaseOrbExpl" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="17" Grid.ColumnSpan="1" Name="tbAspectBaseOrb" Text="tbAspectBaseOrb" FontWeight="Bold" Margin="6"/>
                    <TextBox Grid.Column="1" Grid.Row="17" Grid.ColumnSpan="1" Margin="6" Name="tboxAspectBaseOrb" Text="tboxAspectBaseOrb"/>
                    <TextBlock Grid.Column="2" Grid.Row="17" Grid.ColumnSpan="1" Name="tbMidpointBaseOrb" Text="tbMidpointBaseOrb" FontWeight="Bold" Margin="6"/>
                    <TextBox Grid.Column="3" Grid.Row="17" Grid.ColumnSpan="1" Margin="6" Name="tboxMidpointAspectBaseOrb" Text="tboxMidpointBaseOrb" />

                </Grid>                                
            </TabItem>
            <TabItem Name="tabBasicPoints" Header="TabBasicPoints">
                <Grid Margin="6">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="160*"/>
                        <ColumnDefinition Width="90*"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="160*"/>
                        <ColumnDefinition Width="90*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="80"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="10"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="9" Name="TitleBasicCelPoints" Text="Title Basic CelPoints" Margin="6" Style="{StaticResource subTitleStyle}"/>
                    <TextBlock Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="9" Name="tbBasicExplanation" Text="tbExplanation" Margin="6" TextWrapping="Wrap"/>

                    <Rectangle Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="4"  Grid.RowSpan="9" />
                    <Rectangle Grid.Column="5" Grid.Row="2" Grid.ColumnSpan="4"  Grid.RowSpan="5" />
                    <Rectangle Grid.Column="0" Grid.Row="12" Grid.ColumnSpan="4"  Grid.RowSpan="5" />
                    <Rectangle Grid.Column="5" Grid.Row="12" Grid.ColumnSpan="4"  Grid.RowSpan="3" />
                    
                    <TextBlock Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="4" Name="tbClassical" Text="tbClassical" FontSize="18.0" FontWeight="Bold" Margin="6"/>
                    <TextBlock Grid.Column="5" Grid.Row="2" Grid.ColumnSpan="4" Name="tbModern" Text="tbModern" FontSize="18.0" FontWeight="Bold" Margin="6"/>
                    <TextBlock Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="3" Name="tbBasicPointLeft" Text="tbPoint" Margin="6"/>
                    <TextBlock Grid.Column="3" Grid.Row="3" Name="tbBasicOrbFactorLeft" Text="tbOrbFactor" Margin="6"/>
                    <TextBlock Grid.Column="5" Grid.Row="3" Grid.ColumnSpan="3" Name="tbBasicPointRight" Text="tbPoint" Margin="6"/>
                    <TextBlock Grid.Column="8" Grid.Row="3" Name="tbBasicOrbFactorRight" Text="tbOrbFactor" Margin="6"/>

                    <!-- Sun -->
                    <CheckBox Grid.Column="0" Grid.Row="4" Name="cboxSun" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="4" Name="tbGlyphSun" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="4" Name="tbTextSun" Text="tbSun" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="4" Name="tboxSunFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Moon -->
                    <CheckBox Grid.Column="0" Grid.Row="5" Name="cboxMoon" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="5" Name="tbGlyphMoon" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="5" Name="tbTextMoon" Text="tbMoon" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="5" Name="tboxMoonFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Mercury -->
                    <CheckBox Grid.Column="0" Grid.Row="6" Name="cboxMercury" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="6" Name="tbGlyphMercury" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="6" Name="tbTextMercury" Text="tbMercury" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="6" Name="tboxMercuryFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Venus -->
                    <CheckBox Grid.Column="0" Grid.Row="7" Name="cboxVenus" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="7" Name="tbGlyphVenus" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="7" Name="tbTextVenus" Text="tbVenus" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="7" Name="tboxVenusFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Mars -->
                    <CheckBox Grid.Column="0" Grid.Row="8" Name="cboxMars" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="8" Name="tbGlyphMars" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="8" Name="tbTextMars" Text="tbMars" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="8" Name="tboxMarsFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Jupiter -->
                    <CheckBox Grid.Column="0" Grid.Row="9" Name="cboxJupiter" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="9" Name="tbGlyphJupiter" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="9" Name="tbTextJupiter" Text="tbJupiter" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="9" Name="tboxJupiterFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Saturn -->
                    <CheckBox Grid.Column="0" Grid.Row="10" Name="cboxSaturn" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="10" Name="tbGlyphSaturn" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="10" Name="tbTextSaturn" Text="tbSaturn" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="10" Name="tboxSaturnFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Uranus -->
                    <CheckBox Grid.Column="5" Grid.Row="4" Name="cboxUranus" VerticalAlignment="Center" Margin="6,0,0,0"/>
                    <TextBlock Grid.Column="6" Grid.Row="4" Name="tbGlyphUranus" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="7" Grid.Row="4" Name="tbTextUranus" Text="tbUranus" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="8" Grid.Row="4" Name="tboxUranusFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Neptune -->
                    <CheckBox Grid.Column="5" Grid.Row="5" Name="cboxNeptune" VerticalAlignment="Center" Margin="6,0,0,0"/>
                    <TextBlock Grid.Column="6" Grid.Row="5" Name="tbGlyphNeptune" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="7" Grid.Row="5" Name="tbTextNeptune" Text="tbNeptune" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="8" Grid.Row="5" Name="tboxNeptuneFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Pluto -->
                    <CheckBox Grid.Column="5" Grid.Row="6" Name="cboxPluto" VerticalAlignment="Center" Margin="6,0,0,0"/>
                    <TextBlock Grid.Column="6" Grid.Row="6" Name="tbGlyphPluto" Text="x" FontFamily="EnigmaAstrology" Margin="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="7" Grid.Row="6" Name="tbTextPluto" Text="tbPluto" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="8" Grid.Row="6" Name="tboxPlutoFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />

                    <TextBlock Grid.Column="0" Grid.Row="12" Grid.ColumnSpan="4" Name="tbMundanePoints" Text="tbMundanePoints" FontSize="18.0" FontWeight="Bold" Margin="6"/>
                    <TextBlock Grid.Column="5" Grid.Row="12" Grid.ColumnSpan="4" Name="tbArabicParts" Text="tbArabicParts" FontSize="18.0" FontWeight="Bold" Margin="6"/>

                    <!-- Mc -->
                    <CheckBox Grid.Column="0" Grid.Row="13" Name="cboxMc" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="13" Name="tbGlyphMc" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="13" Name="tbTextMc" Text="tbMc" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="13" Name="tboxMcFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Ascendant -->
                    <CheckBox Grid.Column="0" Grid.Row="14" Name="cboxAsc" IsEnabled="False" IsChecked="True" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="14" Name="tbGlyphAsc" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="14" Name="tbTextAsc" Text="tbAsc" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="14" Name="tboxAscFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Vertex -->
                    <CheckBox Grid.Column="0" Grid.Row="15" Name="cboxVertex" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="15" Name="tbGlyphVertex" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="15" Name="tbTextVertex" Text="tbVertex" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="15" Name="tboxVertexFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Eastpoint -->
                    <CheckBox Grid.Column="0" Grid.Row="16" Name="cboxEastpoint" Margin="6,0,0,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Grid.Row="16" Name="tbGlyphEastpoint" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="16" Name="tbTextEastpoint" Text="tbEastpoint" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="16" Name="tboxEastpointFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />


                    <!-- Pars Fortunae with sect -->
                    <CheckBox Grid.Column="5" Grid.Row="13" Name="cboxParsSect" VerticalAlignment="Center" Margin="6,0,0,0"/>
                    <TextBlock Grid.Column="6" Grid.Row="13" Name="tbGlyphParsSect" Text="x" FontFamily="EnigmaAstrology" Margin="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="7" Grid.Row="13" Name="tbTextParsSect" Text="tbParsSect" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="8" Grid.Row="13" Name="tboxParsSectFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Pars Fortunae no sect -->
                    <CheckBox Grid.Column="5" Grid.Row="14" Name="cboxParsNoSect" VerticalAlignment="Center" Margin="6,0,0,0"/>
                    <TextBlock Grid.Column="6" Grid.Row="14" Name="tbGlyphParsNoSect" Text="x" FontFamily="EnigmaAstrology" Margin="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="7" Grid.Row="14" Name="tbTextParsNoSect" Text="tbParsNoSect" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="8" Grid.Row="14" Name="tboxParsNoSectFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />




                </Grid>
            </TabItem>
            <TabItem Name="tabMathMinorPoints" Header="TabMathMinorPoints">
                <Grid Margin="6">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="160"/>
                        <ColumnDefinition Width="90"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="160"/>
                        <ColumnDefinition Width="90"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="10"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                    </Grid.RowDefinitions>

                    <Rectangle Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="4"  Grid.RowSpan="9" />
                    <Rectangle Grid.Column="5" Grid.Row="2" Grid.ColumnSpan="4"  Grid.RowSpan="5" />
                    <Rectangle Grid.Column="0" Grid.Row="12" Grid.ColumnSpan="4"  Grid.RowSpan="10" />
                    <Rectangle Grid.Column="5" Grid.Row="12" Grid.ColumnSpan="4"  Grid.RowSpan="7" />

                    <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="9" Name="TitleMathMinorPoints" Text="Title MathMinor Points" Margin="6" Style="{StaticResource subTitleStyle}"/>
                    <TextBlock Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="9" Name="tbMathMinorExplanation" Text="tbExplanation" Margin="6" TextWrapping="Wrap"/>
                    <TextBlock Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="4" Name="tbMathematical" Text="tbMathematical" FontSize="18.0" FontWeight="Bold" Margin="6"/>
                    <TextBlock Grid.Column="5" Grid.Row="2" Grid.ColumnSpan="4" Name="tbCentaurs" Text="tbCentaurs" FontSize="18.0" FontWeight="Bold" Margin="6"/>
                    <TextBlock Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="3" Name="tbMathMinorPointLeft" Text="tbPoint" Margin="6"/>
                    <TextBlock Grid.Column="3" Grid.Row="3" Name="tbMathMinorOrbFactorLeft" Text="tbOrbFactor" Margin="6"/>
                    <TextBlock Grid.Column="5" Grid.Row="3" Grid.ColumnSpan="3" Name="tbMathMinorPointRight" Text="tbPoint" Margin="6"/>
                    <TextBlock Grid.Column="8" Grid.Row="3" Name="tbMathMinorOrbFactorRight" Text="tbOrbFactor" Margin="6"/>

                    <!-- Mean Node -->
                    <CheckBox Grid.Column="0" Grid.Row="4" Name="cboxMeanNode" VerticalAlignment="Center" Margin="6,0,0,0" />
                    <TextBlock Grid.Column="1" Grid.Row="4" Name="tbGlyphMeanNode" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="4" Name="tbTextMeanNode" Text="Mean Node" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="4" Name="tboxMeanNodeFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- True Node -->
                    <CheckBox Grid.Column="0" Grid.Row="5" Name="cboxTrueNode" VerticalAlignment="Center" Margin="6,0,0,0" />
                    <TextBlock Grid.Column="1" Grid.Row="5" Name="tbGlyphTrueNode" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="5" Name="tbTextTrueNode" Text="tbTrueNode" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="5" Name="tboxTrueNodeFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- 0 Aries -->
                    <CheckBox Grid.Column="0" Grid.Row="6" Name="cboxZeroAries" VerticalAlignment="Center" Margin="6,0,0,0" />
                    <TextBlock Grid.Column="1" Grid.Row="6" Name="tbGlyphZeroAries" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="6" Name="tbTextZeroAries" Text="tbZeroAries" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="6" Name="tboxZeroAriesFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Mean Black Moon -->
                    <CheckBox Grid.Column="0" Grid.Row="7" Name="cboxMeanBlackMoon" VerticalAlignment="Center" Margin="6,0,0,0" />
                    <TextBlock Grid.Column="1" Grid.Row="7" Name="tbGlyphMeanBLackMoon" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="7" Name="tbTextMeanBlackMoon" Text="tbMeanBlackMoon" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="7" Name="tboxMeanBlackMoonFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Corrected Black Moon -->
                    <CheckBox Grid.Column="0" Grid.Row="8" Name="cboxCorrBlackMoon" VerticalAlignment="Center" Margin="6,0,0,0" />
                    <TextBlock Grid.Column="1" Grid.Row="8" Name="tbGlyphCorrBlackMoon" Text="x" FontFamily="EnigmaAstrology" Margin ="6,0,6,0" VerticalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Grid.Row="8" Name="tbTextCorrBlackMoon" Text="tbCorrectedBlackMoon" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="3" Grid.Row="8" Name="tboxCorrBlackMoonFactor" Text="1.0" Height="16.0" Margin="0,0,6,0" VerticalAlignment="Center" />
                    <!-- Corrected Black Moon Duval -->
                    <CheckBox Grid.Column="0" Grid.Row="9" Name="cboxDuvalBlackMoon" VerticalAlignment="Center" Margin="6,0,0,0" />
